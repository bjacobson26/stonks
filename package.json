{
	"name": "stonks",
	"displayName": "Stonks",
	"description": "Get stock quotes from your editor!",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:stonks-watchlist",
		"onCommand:extension.stonksGetQuote",
		"onCommand:extension.stonksAddToWatchlist",
		"onCommand:extension.stonksRemoveFromWatchlist"
	],
	"repository": {
		"url": "https://github.com/bjacobson26/stonks"
	},
	"publisher": "vsc-stonks",
	"keywords": [
		"stocks",
		"stock",
		"stock market",
		"stonk",
		"stonks"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"view/title": [
				{
					"command": "stonks-watchlist.refreshWatchlist",
					"when": "view == stonks-watchlist",
					"group": "navigation"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "stonks",
					"title": "Stonks",
					"icon": "resources/stock.svg"
				}
			]
		},
		"views": {
			"stonks": [
				{
					"id": "stonks-watchlist",
					"name": "WATCHLIST"
				}
			]
		},
		"commands": [
			{
				"command": "extension.stonksGetQuote",
				"title": "Stonks: Get Quote"
			},
			{
				"command": "extension.stonksAddToWatchlist",
				"title": "Stonks: Add To Watchlist"
			},
			{
				"command": "extension.stonksRemoveFromWatchlist",
				"title": "Stonks: Remove from Watchlist"
			},
			{
				"command": "stonks-watchlist.refreshWatchlist",
				"title": "Stonks: Refresh Watchlist",
				"icon": {
					"light": "resources/refresh.svg",
					"dark": "resources/refresh.svg"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jest": "25.2.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"jest": "25.3.0",
		"ts-jest": "25.3.1",
		"typescript": "3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"lodash": "4.17.15",
		"yahoo-finance": "0.3.6"
	}
}
